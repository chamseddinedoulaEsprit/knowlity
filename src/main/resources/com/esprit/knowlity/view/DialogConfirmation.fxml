<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="rootPane" styleClass="confirmation-dialog-root" stylesheets="@../../../../style/customdialog.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.knowlity.controller.DialogConfirmationController">
    <VBox alignment="CENTER" maxHeight="240.0" maxWidth="360.0" spacing="25.0" styleClass="confirmation-dialog-vbox">
        <ImageView fx:id="iconView" fitHeight="80.0" fitWidth="80.0" styleClass="confirmation-dialog-icon">
            <image>
                <Image url="@../../../../images/delete.png" />
            </image>
        </ImageView>
        <Label fx:id="titleLabel" styleClass="confirmation-dialog-title" />
        <Text fx:id="messageLabel" styleClass="confirmation-dialog-message" wrappingWidth="300.0" />
        <HBox alignment="CENTER" spacing="20.0">
            <Button fx:id="cancelButton" prefHeight="49.0" prefWidth="134.0" styleClass="confirmation-dialog-cancel-button" text="Cancel" />
            <Button fx:id="okButton" prefHeight="49.0" prefWidth="144.0" styleClass="confirmation-dialog-ok-button" text="Confirm" />
        </HBox>
    </VBox>
</StackPane>
