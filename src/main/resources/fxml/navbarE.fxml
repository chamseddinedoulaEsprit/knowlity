<VBox alignment="CENTER" prefHeight="331.0" prefWidth="161.0" spacing="30.0" style="-fx-background-color: linear-gradient(to right, #00b4db, #0083b0); -fx-padding: 15px;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NavbarE">
         <children>
            <ImageView fx:id="test" fitHeight="88.0" fitWidth="147.0" preserveRatio="true">
               <image>
                  <Image url="@../images/knowlity.png" />
               </image>
            </ImageView>
            <Label style="-fx-text-fill: WHITE; -fx-font-size: 24px; -fx-font-weight: bold; -fx-padding: 0 0 0 10;" text="KnowLity">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button onAction="#home" style="-fx-background-color: transparent; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px;" text="Home" />
            <Button fx:id="eventListId" onAction="#eventListId" style="-fx-background-color: transparent; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px;" text="Event" />
            <MenuButton style="-fx-background-color: transparent; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px;" text="Courses">
               <items>
                  <MenuItem onAction="#AllCourses" text="All courses" />
                  <MenuItem onAction="#AddCourse" text="Add course" />
               </items>
            </MenuButton>
            <Button onAction="#logout" style="-fx-background-color: transparent; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 14px;" text="Logout">
         <VBox.margin>
            <Insets bottom="150.0" />
